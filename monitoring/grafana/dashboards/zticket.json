    {
      "id": 19,
      "title": "HikariCP Pool 포화도",
      "description": "pending > 0이 지속되면 커넥션 풀 부족. pending 급증은 timeout 직전 경고 신호",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 48 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "hikaricp_connections_active{application=\"zticket\"} / hikaricp_connections_max{application=\"zticket\"}",
          "legendFormat": "사용률 (active/max)"
        },
        {
          "refId": "B",
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "hikaricp_connections_pending{application=\"zticket\"}",
          "legendFormat": "pending (대기 중)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "lineWidth": 1, "fillOpacity": 10, "showPoints": "never" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 0.8 },
              { "color": "red", "value": 1 }
            ]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "사용률 (active/max)" },
            "properties": [{ "id": "unit", "value": "percentunit" }, { "id": "max", "value": 1 }]
          },
          {
            "matcher": { "id": "byName", "options": "pending (대기 중)" },
            "properties": [{ "id": "custom.axisPlacement", "value": "right" }, { "id": "unit", "value": "short" }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 18,
      "title": "HikariCP Connection Timeout",
      "description": "동시 VT 수 > 커넥션 풀 크기일 때 timeout 발생",
